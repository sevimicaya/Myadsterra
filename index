<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Redirect Example</title>
    <script>
        // This function redirects to the first URL and then opens a new tab with the second URL
        function redirect() {
            // Open the second URL in a new tab
            window.open('https://www.profitablecpmrate.com/bvtvhc217v?key=eacfade708e6029fdade7c6f6efc9e17', '_blank');
            
            // Redirect to the first URL in the current tab
            window.location.href = 'https://aowvrr.romanceafairs.net/?utm_source=da57dc555e50572d&s1=218971&s2=2024081&click_id=ADSTERRA&ban=other&j1=1';
        }
    </script>
</head>
<body onload="redirect()">
    <h1>Redirecting...</h1>
</body>
</html>
